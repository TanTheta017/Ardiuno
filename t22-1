int potval;
int buzzpin=8;
int potpin=A3;
int dt=100;

void setup()
{
 Serial.begin(9600);
  pinMode(buzzpin,OUTPUT);
  pinMode(potpin,INPUT);
}

void loop()
{potval=analogRead(potpin);
 Serial.println(potval); 
 while(potval>900)
  {digitalWrite(buzzpin,HIGH);
   potval=analogRead(potpin);
   Serial.println(potval);}
 
  digitalWrite(buzzpin,LOW);
}
