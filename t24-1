int buzzpin=13;
int potpin=A1;
int potval;
int toneval;

void setup()
{
  pinMode(buzzpin, OUTPUT);
  pinMode(potpin,INPUT);
}

void loop()
{
  potval=analogRead(potpin);
  toneval=(9940./1023.)*potval+60;
  digitalWrite(buzzpin, HIGH);
  delayMicroseconds(toneval); 
  digitalWrite(buzzpin, LOW);
  delayMicroseconds(toneval);
}
