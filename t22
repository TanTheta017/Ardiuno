int mynum;
int buzzpin=8;
String msg="Input number: ";
int dt=2000;
void setup()
{
 Serial.begin(9600);
  pinMode(buzzpin,OUTPUT);
}

void loop()
{
 Serial.println(msg);
  while(Serial.available()==0){}
  mynum=Serial.parseInt();
  if(mynum>10)
  {digitalWrite(buzzpin,HIGH);
  delay(dt);
  digitalWrite(buzzpin,LOW);}
}
